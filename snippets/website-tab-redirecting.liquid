{% style %}
  .website-tab{
  	background: #ebebeb;
  }
  .website-tab__Wrapper{
	max-width: 1200px;
	padding: 0 50px;
	margin: auto;
	height:33px;
  }
  .website-tab-img{
  	display: inline-flex;
	justify-content:center;
	align-items: center;
	height: 100%;
	padding: 5px 10px;
  }
  .website-tab-img img{
  }
  .website-tab-img.active{
  	background: #fff;

  }
  .website-tab-img.inactive{
    margin-left: -4px;
  	background: #5b5b5b;
    -webkit-box-shadow: inset 11px -11px 17px -13px #151515; 
    box-shadow: inset 11px -11px 17px -13px #151515;
    transition: all 0.3s ease;
  }
  .website-tab-img.inactive:hover,
  .website-tab-img.inactive:focus{
  	background: #3a3c3c;
  }
  @media (max-width: 667px){
    .website-tab{
    	text-align: center;
    }
  }
{% endstyle %}

<div class="website-tab">
  <div class="website-tab__Wrapper">
    <div class="website-tab-img active">
      <img width= "120px" src="https://cdn.shopify.com/s/files/1/2451/6611/files/FinalLogoMainBlackWithoutSubText_4186d8fc-26ba-4c03-b4ba-ed773fe7d6ed.png?v=1646748231">
    </div>
    <div class="website-tab-img inactive">
      <a href="https://therapiewelt.de/" target="_blank">
        <img width= "100px" src="https://cdn.shopify.com/s/files/1/2451/6611/files/FinalLogoMainWhiteWithoutText.png?v=1646749243">
      </a>
    </div>
  </div>
</div>