    "1629465806e866cd2d": {
      "type": "impact-outcome",
      "blocks": {
        "b9612911-78b1-4ec0-9eb1-362f7d45c3ef": {
          "type": "blocks_outcome",
          "settings": {
            "icon_span": "<span class=\"icon-Icons_Sleep01\"><span class=\"path1\"><\/span><span class=\"path2\"><\/span><span class=\"path3\"><\/span><span class=\"path4\"><\/span><span class=\"path5\"><\/span><span class=\"path6\"><\/span><span class=\"path7\"><\/span><span class=\"path8\"><\/span><span class=\"path9\"><\/span><span class=\"path10\"><\/span><span class=\"path11\"><\/span><span class=\"path12\"><\/span><\/span>",
            "title-icon": "Für einen besseren Schlaf",
            "content": "Therapiedecken haben eine entspannende Wirkung auf den Körper und fördern schnelleres Einschlafen. Durch das Auflegen der Decke wird die Ausschüttung des Schlafhormons Melatonin gefördert. Besserer Schlaf wirkt sich positiv auf Alltag und Arbeit aus."
          }
        },
        "198c972f-c489-4358-8f56-0a406d477eac": {
          "type": "blocks_outcome",
          "settings": {
            "icon_span": "<span class=\"icon-Icons_Stress01 icons21_font_size\"><span class=\"path1\"><\/span><span class=\"path2\"><\/span><span class=\"path3\"><\/span><span class=\"path4\"><\/span><span class=\"path5\"><\/span><span class=\"path6\"><\/span><span class=\"path7\"><\/span><span class=\"path8\"><\/span><span class=\"path9\"><\/span><span class=\"path10\"><\/span><span class=\"path11\"><\/span><\/span>",
            "title-icon": "Zur Reduzierung von Stress",
            "content": "Gewicht begünstigt das Abschalten und Entspannen des Körpers und Geistes nach stressigen und unruhigen Tagen. Durch den Druck der Decke auf den Körper wird der Cortisolspiegel gesenkt (sog. Stresshormon). Die Muskeln und das Nervensystem sind nun entspannt."
          }
        },
        "2dbe9fc2-5360-4a42-82cd-3f6127123c64": {
          "type": "blocks_outcome",
          "settings": {
            "icon_span": "<span class=\"icon-Icons_WellBeing01 icons21_font_size\"><span class=\"path1\"><\/span><span class=\"path2\"><\/span><span class=\"path3\"><\/span><span class=\"path4\"><\/span><span class=\"path5\"><\/span><span class=\"path6\"><\/span><span class=\"path7\"><\/span><span class=\"path8\"><\/span><span class=\"path9\"><\/span><span class=\"path10\"><\/span><span class=\"path11\"><\/span><span class=\"path12\"><\/span><span class=\"path13\"><\/span><span class=\"path14\"><\/span><\/span>",
            "title-icon": "Für ein besseres Wohlbefinden",
            "content": "Das Einsetzen einer Gewichtsdecke kann dabei helfen, den Körper schneller herunterzufahren und in den Ruhemodus zu befördern. Das Gewicht der Decke wirkt auf die Muskeln, Sehnen und Gelenke, sodass der Körper intensiver wahrgenommen wird."
          }
        }
