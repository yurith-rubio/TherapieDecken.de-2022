    "1629467370a1305632": {
      "type": "product-details",
      "blocks": {
        "2ff3c74b-41b4-4aab-bf79-5d8afe927bc3": {
          "type": "info-product-details",
          "settings": {
            "title_block": "Gewicht",
            "html_content": "<p>Wir empfehlen eine Therapiedecke auszuwählen, die ungefähr 10% Ihres Körpergewichts ausmacht. Unsicher?\n              Verwenden Sie diese Deckengewichtstabelle:<\/p>\n            <div class=\"deckengewicht-wrap\">\n              <table align=\"center\" class=\"deckengewicht-tg\">\n                <tbody>\n                  <tr class=\"deckengewicht-tr\">\n                    <th class=\"tg-c6of\"><\/th>\n                    <th class=\"tg-rg0h\">30 - 49 kg<\/th>\n                    <th class=\"tg-rg0h\">50 - 69 kg<\/th>\n                    <th class=\"tg-rg0h\">70 - 89 kg<\/th>\n                    <th class=\"tg-rg0h\">90 - 109 kg<\/th>\n                    <th class=\"tg-rg0h\">110+ kg<\/th>\n                  <\/tr>\n                  <tr class=\"deckengewicht-tr\">\n                    <td class=\"tg-4ir0\">135 x 200 cm<\/td>\n                    <td class=\"tg-amwm\">4 kg<\/td>\n                    <td class=\"tg-amwm\">6 kg<\/td>\n                    <td class=\"tg-amwm\">8 kg<\/td>\n                    <td class=\"tg-amwm\">10 kg<\/td>\n                    <td class=\"tg-amwm\">12 kg<\/td>\n                  <\/tr>\n                  <tr class=\"deckengewicht-tr\">\n                    <td class=\"tg-4ir0\">155 x 220 cm<\/td>\n                    <td class=\"tg-amwm\">6 kg<\/td>\n                    <td class=\"tg-amwm\">8 kg<\/td>\n                    <td class=\"tg-amwm\">10 kg<\/td>\n                    <td class=\"tg-amwm\" colspan=\"2\">12 kg<\/td>\n                  <\/tr>\n                <\/tbody>\n              <\/table>\n            <\/div>",
            "extra_block": false,
            "subtitle_block": "Place your title",
            "html_content_extra": "ONLY IF Extra Content 1 - Place here your content",
            "extra_block2": false,
            "subtitle_block2": "Place your title",
