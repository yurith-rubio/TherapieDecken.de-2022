    "16540972822e9c1533": {
      "type": "expert-testimonials-left",
      "blocks": {
        "481ac8ac-e0db-49dc-994e-34c3e5a27c66": {
          "type": "testimonial",
          "settings": {
            "link": "https:\/\/therapiedecken.de\/pages\/diana-zur-lowens-erfahrung-mit-therapiedecken",
            "tmex_picture": "shopify:\/\/shop_images\/diana_zur_lowen.jpg",
            "tmex_name": "Diana zur Löwen",
            "tmex_role": "Unternehmerin & Influencerin",
            "tmex_paragraph": "<p>Man fühlt sich einfach geborgen und wiein Schutz genommen.  Das hilft mir besonderes, wenn ich wieder sehr nachdenklich und unruhig bin...<\/p>"
          }
        },
        "50240af9-3242-4fea-8cb0-a6db4379e089": {
          "type": "testimonial",
          "settings": {
            "link": "https:\/\/therapiedecken.de\/pages\/markus-rupperts-erfahrung-mit-therapiedecken",
            "tmex_picture": "shopify:\/\/shop_images\/markus_ruppert.jpg",
            "tmex_name": "Markus Ruppert",
            "tmex_role": "Heilpraktiker & Traumatherapeut",
            "tmex_paragraph": "<p>Für mich ist die Decke weder privat noch in meiner Arbeit wegzudenken. Es ist ein wichtiges Werkzeug und eine  wertvolle Ressource.<\/p>"
          }
        },
        "644d0c13-97ca-49e0-b4c1-0c2ece163060": {
          "type": "testimonial",
          "settings": {
            "link": "https:\/\/therapiedecken.de\/pages\/caroline-helmings-erfahrung-mit-therapiedecken",
            "tmex_picture": "shopify:\/\/shop_images\/caroline_helming.jpg",
            "tmex_name": "Caroline Helming",
            "tmex_role": "Ergotherapeutin & Schlafberaterin",
            "tmex_paragraph": "<p>Ich bin ein großer Fan des Konzepts. Ob privat oder beruflich, ich kann sie uneingeschränkt weiterempfehlen.<\/p>"
          }
        },
        "b36c3480-f257-4644-b8e4-84516d0723de": {
          "type": "testimonial",
          "settings": {
            "link": "https:\/\/therapiedecken.de\/pages\/dinah-bergers-erfahrung-mit-therapiedecken",
            "tmex_picture": "shopify:\/\/shop_images\/dinah_berger.jpg",
            "tmex_name": "Dinah Berger",
            "tmex_role": "Psychologin",
            "tmex_paragraph": "<p>Während Ängste gemildert werden, führt der Tiefendruck zu einem Gefühl von Sicherheit, das gerade bei Menschen mit Traumata oft fehlt.<\/p>"
          }
        },
        "fdf8e9f7-ad6f-4205-a688-101323511069": {
          "type": "testimonial",
          "settings": {
            "link": "https:\/\/therapiedecken.de\/pages\/michaela-gross-erfahrung-mit-therapiedecken",
            "tmex_picture": "shopify:\/\/shop_images\/Michaela_Gross_10_1_4.jpg",
            "tmex_name": "Michaela Gross",
            "tmex_role": "Heilpraktikerin für Psychotherapie",
            "tmex_paragraph": "<p>Als Traumatherapeutin empfehle ich die Decken bei Schlaf-\/Angst- und Panikstörungen. Es hilft, dass sich die Patienten wieder spüren können.<\/p>"
          }
        },
        "9d554a42-272f-4982-9633-8aaadd46420a": {
          "type": "testimonial",
          "settings": {
            "link": "https:\/\/therapiedecken.de\/pages\/dr-sabine-hoffmanns-erfahrung-mit-therapiedecken",
            "tmex_picture": "shopify:\/\/shop_images\/phsycologist-therapiedecken-img1.jpg",
            "tmex_name": "Dr. Sabine Hoffmann",
            "tmex_role": "Leitende Psychologin",
            "tmex_paragraph": "<p>Diese Decken können ein spontanes Gefühl von Ruhe, kurz gesagt „alles ist gut” auf der körperlichen und damit auch psychischen Ebene auslösen.<\/p>"
          }
        },
        "399c06f9-bd66-4a95-a533-a38d743eb7f7": {
          "type": "testimonial",
          "settings": {
            "link": "https:\/\/therapiedecken.de\/pages\/katharina-meier-batrakows-erfahrung-mit-therapiedecken",
            "tmex_picture": "shopify:\/\/shop_images\/tm21_pic.jpg",
            "tmex_name": "Katharina Meier-Batrakow",
            "tmex_role": "Psychologin für Kinder- und Jugendliche",
            "tmex_paragraph": "<p>Jemanden zu umarmen, um Trost zu spenden, ein Kind zu halten, um es zu beruhigen – all das sind altbewährte Methoden zur Stressreduktion.<\/p>"
          }
        },
        "de783f0c-db2d-41a7-bb17-6855c561bb43": {
          "type": "testimonial",
          "settings": {
            "link": "https:\/\/therapiedecken.de\/pages\/carolin-weigands-erfahrung-mit-therapiedecken",
            "tmex_picture": "shopify:\/\/shop_images\/tm21_pic_4.jpg",
            "tmex_name": "Carolin Weigand",
            "tmex_role": "Sozialpädagogin & Erzieherin",
            "tmex_paragraph": "<p>Oft wird mir berichtet, dass sich Schüler nur schwer konzentrieren können. Gerne empfehle ich dann die Therapiedecke um runterzufahren.<\/p>"
          }
        },
        "817e1f13-a792-47f3-9c92-db140b44e961": {
          "type": "testimonial",
          "settings": {
            "link": "https:\/\/therapiedecken.de\/pages\/christine-tuchlers-erfahrung-mit-therapiedecken",
            "tmex_picture": "shopify:\/\/shop_images\/Tuchler_stehend_1_1.jpg",
            "tmex_name": "Christine Tuchler",
            "tmex_role": "Psychotherapeutin für Kinder & Jugendliche",
            "tmex_paragraph": "<p>Es ist einfach schön mit anzusehen wie nach kurzer Zeit die motorische Unruhe des Patienten zurückgeht.<\/p>"
          }
        }
      },
